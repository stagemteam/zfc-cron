{
  "name": "stagem/zfc-cron",
  "description": "ZF cron module based on https://github.com/yalesov/zf2-cron",
  "type": "zf-module",
  "license": "MIT",
  "keywords": [
    "zf",
    "zf3",
    "cron"
  ],
  "authors": [
    {
      "name": "Vlad Kozak",
      "email": "vlad.gem.typ@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Serhii Popov",
      "email": "popow.serhii@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6 || ^7.",
    "yalesov/zf2-cron": "3.*"
  },
  "autoload": {
    "psr-4": {
      "Stagem\\ZfcCron\\": "src/"
    }
  },
  "extra": {
    "zf": {
      "config-provider": "Stagem\\ZfcCron\\ConfigProvider",
      "component": "Stagem\\ZfcCron"
    }
  }
}
